extends ../../layouts/default

block content
	.container-sm: .pv-2
		h1!=errorTitle || 'Not found (404)'
		if 'object' == typeof errorMsg
			div(style='margin: 30px 0;').debug!= htmlStringify(errorMsg)
		else
			p.lead!= utils.textToHTML(errorMsg) || "We're sorry, but the page you are looking for does not exist"
		p: a(href='/').btn.btn-primary.btn-lg D@W-LA Home
